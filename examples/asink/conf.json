{
    "tasks" : {
        "clone-asink" : {
            "command" : "git",
            "args"    : [
                "clone",
                "https://github.com/GroundSix/asink.git"
            ]
        },
        "build-asink-deps" : {
            "dir"     : "asink",
            "command" : "make",
            "args"    : [
                "deps"
            ],
            "require" : "clone-asink"
        }
    }
}